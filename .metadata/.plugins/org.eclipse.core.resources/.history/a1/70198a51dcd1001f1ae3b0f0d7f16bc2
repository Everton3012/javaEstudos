package fundamentos;

public class AreaCircunferencia {

	public static void main(String[] args) {
		double raio = 3.4;
		final double PI = 3.14159;
		
		
		double area1 = PI *raio * raio;
		
		System.out.println(area1);
		
		raio = 10;
		double area = PI * raio * raio; 
		System.out.println(area);
	}
	
}
